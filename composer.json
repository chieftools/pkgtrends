{
    "name": "irongate/pkgtrends",
    "description": "A quick way to compare package downloads across languages.",
    "keywords": [
        "package",
        "trends",
        "packagist",
        "npm",
        "pypi",
        "hex",
        "wordpress"
    ],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "4.2.*",
        "google/cloud-bigquery": "1.12.*",
        "guzzlehttp/guzzle": "6.3.*",
        "laravel/framework": "6.2.*",
        "laravel/helpers": "1.1.*",
        "laravel/tinker": "1.0.*",
        "laravel/vapor-core": "2.0.*",
        "predis/predis": "1.1.*",
        "ramsey/uuid": "3.8.*",
        "sentry/sentry-laravel": "1.3.*"
    },
    "require-dev": {
        "facade/ignition": "1.11.*",
        "laravel/vapor-cli": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "IronGate\\Pkgtrends\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
