{
    "name": "irongate/packagetrends",
    "description": "A quick way to compare package downloads across languages.",
    "keywords": [
        "package",
        "trends",
        "packagist",
        "npm",
        "pypi",
        "hex",
        "wordpress"
    ],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "4.0.*",
        "google/cloud-bigquery": "1.4.*",
        "guzzlehttp/guzzle": "6.3.*",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "1.0.*",
        "predis/predis": "1.1.*",
        "sentry/sentry-laravel": "0.11.*"
    },
    "autoload": {
        "psr-4": {
            "IronGate\\Pkgtrends\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
